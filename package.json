{
  "name": "deliclinkenhanced",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:css": "sass src/styles/index.scss src/output.css && tailwindcss build src/output.css -o src/final.css",
    "watch:sass": "sass --watch src/styles/index.scss:src/output.css",
    "watch:tailwind": "tailwindcss -i ./src/output.css -o ./src/final.css --watch",
    "watch:styles": "npm-run-all --parallel watch:sass watch:tailwind",
    "build:sass": "sass src/styles/index.scss src/output.css",
    "build:tailwind": "tailwindcss -i ./src/output.css -o ./src/final.css",
    "build:styles": "npm-run-all build:sass build:tailwind"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-query-devtools": "^5.51.23",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0"
  }
}
